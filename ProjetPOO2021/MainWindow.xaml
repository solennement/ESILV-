        xmlns:local="clr-namespace:ProjetFinalPoo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Client/Effectif" Click="ClientEffectif"/>
            <Button Content="Commande" Click="Commande" Margin="20,0,0,0"/>
        </StackPanel>
        <Frame Name="Main" Margin="0,35,0,0"/>

    </Grid>
</Window>
